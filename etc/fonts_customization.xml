<fonts-modification version="1">
    <family customizationType="new-named-family" name="aileron">
        <font weight="300" style="normal">Aileron-Light.otf</font>
        <font weight="300" style="italic">Aileron-LightItalic.otf</font>
        <font weight="400" style="normal">Aileron-Regular.otf</font>
        <font weight="400" style="italic">Aileron-Italic.otf</font>
        <font weight="600" style="normal">Aileron-SemiBold.otf</font>
        <font weight="600" style="italic">Aileron-SemiBoldItalic.otf</font>
        <font weight="700" style="normal">Aileron-Bold.otf</font>
        <font weight="700" style="italic">Aileron-BoldItalic.otf</font>
    </family>
    <alias name="aileron-semibold" to="aileron" weight="600" />

    <family customizationType="new-named-family" name="inter">
        <font weight="300" style="normal">Inter-Light.ttf</font>
        <font weight="300" style="italic">Inter-LightItalic.ttf</font>
        <font weight="400" style="normal">Inter-Regular.ttf</font>
        <font weight="400" style="italic">Inter-Italic.ttf</font>
        <font weight="500" style="normal">Inter-Medium.ttf</font>
        <font weight="500" style="italic">Inter-MediumItalic.ttf</font>
        <font weight="700" style="normal">Inter-Bold.ttf</font>
        <font weight="700" style="italic">Inter-BoldItalic.ttf</font>
    </family>
    <alias name="inter-medium" to="inter" weight="500" />
</fonts-modification>
